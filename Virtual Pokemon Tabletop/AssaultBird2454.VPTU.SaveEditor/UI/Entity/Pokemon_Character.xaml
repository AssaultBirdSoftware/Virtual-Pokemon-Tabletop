                            <xctk:IntegerUpDown x:Name="Status_Volatile_Sleep_Duration" Margin="93,75,0,0" Grid.Row="1"
                                                VerticalAlignment="Top" Height="26" Value="0" TextAlignment="Center"
                                                Maximum="9999" Minimum="-1" HorizontalAlignment="Left" Width="89"
                                                IsEnabled="False"
                                                ValueChanged="Status_Volatile_Sleep_Duration_ValueChanged" />
                            <Label Content="Temp Hit Points" HorizontalAlignment="Left" Margin="0,106,0,0"
                                   VerticalAlignment="Top" Grid.Row="1" Height="26" Width="95" />
                            <xctk:IntegerUpDown x:Name="Status_Volatile_TemporaryHitPoints_Value"
                                                HorizontalAlignment="Left" Margin="100,106,0,0" Grid.Row="1"
                                                VerticalAlignment="Top" Height="26" Width="89" Value="0"
                                                TextAlignment="Center" Maximum="9999" Minimum="0" IsEnabled="False"
                                                ValueChanged="Status_Volatile_TemporaryHitPoints_Value_ValueChanged" />
                        </Grid>
                    </GroupBox>
                    <GroupBox Header="Other Status" HorizontalAlignment="Left" Height="437" VerticalAlignment="Top"
                              Width="321" Margin="651,0,0,0">
                        <Grid Margin="0">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="215" />
                                <RowDefinition />
                            </Grid.RowDefinitions>
                            <Button x:Name="Status_Other_ClearAll" Content="Clear All" HorizontalAlignment="Right"
                                    Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Grid.Row="1" />
                            <CheckBox x:Name="Status_Other_Fainted" Content="Fainted" HorizontalAlignment="Left"
                                      Margin="0,10,0,0" VerticalAlignment="Top" Checked="Status_Other_Fainted_Checked"
                                      Unchecked="Status_Other_Fainted_Unchecked" />
                            <CheckBox x:Name="Status_Other_Blindness" Content="Blindness" HorizontalAlignment="Left"
                                      Margin="0,30,0,0" VerticalAlignment="Top"
                                      Checked="Status_Other_Blindness_Checked"
                                      Unchecked="Status_Other_Blindness_Unchecked" />
                            <CheckBox x:Name="Status_Other_TotalBlindness" Content="Total Blindness"
                                      HorizontalAlignment="Left" Margin="0,50,0,0" VerticalAlignment="Top"
                                      Checked="Status_Other_TotalBlindness_Checked"
                                      Unchecked="Status_Other_TotalBlindness_Unchecked" />
                            <CheckBox x:Name="Status_Other_Slowed" Content="Slowed" HorizontalAlignment="Left"
                                      Margin="0,70,0,0" VerticalAlignment="Top" Checked="Status_Other_Slowed_Checked"
                                      Unchecked="Status_Other_Slowed_Unchecked" />
                            <CheckBox x:Name="Status_Other_Stuck" Content="Stuck" HorizontalAlignment="Left"
                                      Margin="0,90,0,0" VerticalAlignment="Top" Checked="Status_Other_Stuck_Checked"
                                      Unchecked="Status_Other_Stuck_Unchecked" />
                            <CheckBox x:Name="Status_Other_Trapped" Content="Trapped" HorizontalAlignment="Left"
                                      Margin="0,110,0,0" VerticalAlignment="Top" Checked="Status_Other_Trapped_Checked"
                                      Unchecked="Status_Other_Trapped_Unchecked" />
                            <CheckBox x:Name="Status_Other_Tripped" Content="Tripped" HorizontalAlignment="Left"
                                      Margin="0,130,0,0" VerticalAlignment="Top" Checked="Status_Other_Tripped_Checked"
                                      Unchecked="Status_Other_Tripped_Unchecked" />
                            <CheckBox x:Name="Status_Other_Vulnerable" Content="Vulnerable" HorizontalAlignment="Left"
                                      Margin="0,150,0,0" VerticalAlignment="Top"
                                      Checked="Status_Other_Vulnerable_Checked"
                                      Unchecked="Status_Other_Vulnerable_Unchecked" />
                            <Label Content="No Options Available for &quot;Other&quot; Status Effects"
                                   Margin="10,10,10,0" Grid.Row="1" VerticalAlignment="Top"
                                   VerticalContentAlignment="Center" HorizontalContentAlignment="Center" />
                        </Grid>
                    </GroupBox>
                </Grid>
            </TabItem>
            <TabItem Header="User Permissions" HorizontalAlignment="Left" Height="20" VerticalAlignment="Top">
                <Grid Background="White">
                    <ListView x:Name="UserList" Margin="0,0,-0.4,0.4">
                        <ListView.View>
                            <GridView>
                                <GridViewColumn Header="View" Width="50px">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <CheckBox Margin="5, 0" IsChecked="{Binding View}" />
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="Edit" Width="50px">
                                    <GridViewColumn.CellTemplate>
                                        <DataTemplate>
                                            <CheckBox Margin="5, 0" IsChecked="{Binding Edit}" />
                                        </DataTemplate>
                                    </GridViewColumn.CellTemplate>
                                </GridViewColumn>
                                <GridViewColumn Header="UserID" Width="100px" DisplayMemberBinding="{Binding ID}" />
                                <GridViewColumn Header="User Name" Width="200px" DisplayMemberBinding="{Binding Name}" />
                                <GridViewColumn Header="Character Name" Width="200px"
                                                DisplayMemberBinding="{Binding ICN}" />
                                <GridViewColumn Header="Groups" Width="300px" DisplayMemberBinding="{Binding Groups}" />
                            </GridView>
                        </ListView.View>
                    </ListView>
                </Grid>
            </TabItem>
        </TabControl>
        <Border x:Name="Token" BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Height="124"
                Margin="854,10,0,0" VerticalAlignment="Top" Width="124" />
        <Button x:Name="Button_SelectToken" Content="Change Token" Margin="854,139,12.4,0" VerticalAlignment="Top"
                Click="Button_SelectToken_Click" />

    </Grid>
</Window>