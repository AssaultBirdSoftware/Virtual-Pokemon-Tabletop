<Window x:Class="AssaultBird2454.VPTU.SaveEditor.UI.Battle.Effect_Script_Editor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssaultBird2454.VPTU.SaveEditor.UI.Battle"
        mc:Ignorable="d"
        Title="Effect Script Editor" Height="429.825" Width="533.333">
    <Grid>
        <Button x:Name="Cancel" Content="Cancel" HorizontalAlignment="Right" Margin="0,0,10,10" VerticalAlignment="Bottom" Width="75" Click="Cancel_Click"/>
        <Button x:Name="Save_Button" Content="Save" HorizontalAlignment="Right" Margin="0,0,90,10" VerticalAlignment="Bottom" Width="75" Click="Save_Click"/>
        <Button x:Name="SaveExit" Content="Save &amp; Exit" HorizontalAlignment="Right" Margin="0,0,170,10" VerticalAlignment="Bottom" Width="75" Click="SaveExit_Click"/>
        <TextBox x:Name="Script" TextWrapping="Wrap" Margin="0,0,0,35" AcceptsTab="True" AcceptsReturn="True" IsTabStop="False"/>

    </Grid>
</Window>
