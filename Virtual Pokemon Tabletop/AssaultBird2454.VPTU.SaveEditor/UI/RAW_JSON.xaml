<Window x:Class="AssaultBird2454.VPTU.SaveEditor.UI.RAW_JSON"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssaultBird2454.VPTU.SaveEditor.UI"
        mc:Ignorable="d"
        Title="Import &amp; Export" Height="468.482" Width="500">
    <Grid>
        <Button x:Name="Cancel_Button" Content="Cancel" HorizontalAlignment="Left" Margin="390,0,0,10" Width="92" Height="19.96" VerticalAlignment="Bottom" Click="Cancel_Button_Click" IsCancel="True"/>
        <Button x:Name="Submit_Button" Content="Import / Restore" HorizontalAlignment="Left" Margin="293,0,0,10" Width="92" Height="19.96" VerticalAlignment="Bottom" Click="Submit_Button_Click" IsDefault="True"/>
        <RichTextBox x:Name="Data_JSON" Margin="0,0,0,34.96">
            <FlowDocument/>
        </RichTextBox>
        <CheckBox x:Name="Option_Overwrite" Content="Overwrite Import" HorizontalAlignment="Left" Margin="10,410,0,0" VerticalAlignment="Top" Unchecked="Option_Overwrite_Unchecked" Checked="Option_Overwrite_Checked" IsEnabled="False"/>

    </Grid>
</Window>
