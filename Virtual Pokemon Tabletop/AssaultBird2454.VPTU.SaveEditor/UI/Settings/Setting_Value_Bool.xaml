<UserControl x:Class="AssaultBird2454.VPTU.SaveEditor.UI.Settings.Setting_Value_Bool"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AssaultBird2454.VPTU.SaveEditor.UI.Settings"
             mc:Ignorable="d" 
             d:DesignHeight="300" Height="26">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition Width="Auto"/>
        </Grid.ColumnDefinitions>
        <Label x:Name="Node_Name" Content="Setting Name"/>
        <CheckBox x:Name="Node_Check" Content="Enabled" VerticalContentAlignment="Center" HorizontalAlignment="Right" Grid.Column="1"/>
    </Grid>
</UserControl>
